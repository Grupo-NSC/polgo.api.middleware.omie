# Configurações do Ambiente
NODE_ENV=development
LOG_LEVEL=debug

# Configurações AWS
AWS_REGION=us-east-1
AWS_STAGE=dev

# URLs dos Serviços Polgo
POLGO_API_URL=https://api-dev.polgo.com.br
POLGO_AUTH_URL=https://auth-dev.polgo.com.br
POLGO_WEBHOOK_URL=https://webhook-dev.polgo.com.br

# Credenciais Omie
OMIE_API_KEY=sua_chave_api_aqui
OMIE_API_SECRET=seu_segredo_api_aqui
OMIE_APP_KEY=sua_app_key_aqui

# Configurações de Retry
MAX_RETRIES=3
RETRY_DELAY=1000

# Configurações de Timeout
REQUEST_TIMEOUT=5000
LAMBDA_TIMEOUT=30

# Configurações de Cache
CACHE_TTL=3600